{
  "name": "OpenBerg Lector",
  "version": "7.0a2",
  "developer": {
    "name": "David Rajchenbach-Teller",
    "url": "http://lector.yoric.rajtel.name"
  },
  "icons": {
    "16": "/lector-dev/img/icon16.png",
    "32": "/lector-dev/img/icon32.png",
    "48": "/lector-dev/img/icon48.png",
    "60": "/lector-dev/img/icon60.png",
    "64": "/lector-dev/img/icon64.png",
    "90": "/lector-dev/img/icon90.png",
    "128": "/lector-dev/img/icon128.png",
    "256": "/lector-dev/img/icon256.png",
    "512": "/lector-dev/img/icon512.png"
  },
  "description": "open-source, open-standards e-Book reader.",
  "launch_path": "/lector-dev/index.html",
  "default_locale": "en",
  "type": "web",
  "fullscreen": "true",
  "activities": {
    "open": {
      "filters": {
        "type": ["application/epub+zip"]
      },
      "href": "/lector-dev/index.html?action=open",
      "disposition": "window",
      "returnValue": "false"
    },
    "view": {
      "filters": {
        "type": ["application/epub+zip"]
      },
      "href": "/lector-dev/index.html?action=view",
      "disposition": "window",
      "returnValue": "false"
    }
  },
  "orientation": ["portrait-primary"],
  "appcache_path": "/lector-dev/manifest.appcache"

  "permissions" : 
  {
    "device-storage:sdcard" : { "access": "readonly" }
  }
}
